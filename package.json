{
  "name": "@z-code/vite-plugin-swc",
  "version": "0.0.0-t",
  "description": "Vite plugin for transforming source files with SWC.",
  "homepage": "https://timtech.blog/posts/transform-typescript-legacy-decorators-vite-swc-plugin/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ziir/vite-plugin-swc-transform.git"
  },
  "author": {
    "name": "Zero <github.com/z-nerd>"
  },
  "license": "MIT",
  "keywords": [
    "vite",
    "plugin",
    "vite-plugin",
    "swc",
    "transform",
    "typescript"
  ],
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "dist/index.umd.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "scripts": {
    "build": "tsc && vite build"
  },
  "devDependencies": {
    "path": "^0.12.7",
    "typescript": "^5.3.3",
    "url": "^0.11.3",
    "vite": "^5.0.10",
    "vite-plugin-dts": "^3.8.1"
  },
  "peerDependencies": {
    "vite": "^4 || ^5"
  },
  "dependencies": {
    "@swc/core": "^1.4.11"
  }
}
