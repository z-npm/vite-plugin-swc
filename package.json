{
  "name": "@z-code/vite-plugin-swc",
  "version": "0.3.3",
  "description": "Vite plugin for transforming source files with SWC.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/z-npm/vite-plugin-swc.git"
  },
  "author": {
    "name": "Zero <github.com/z-nerd>"
  },
  "license": "MIT",
  "keywords": [
    "vite",
    "plugin",
    "vite-plugin",
    "swc",
    "top level await",
    "transform",
    "typescript",
    "stage 3 decorators"
  ],
  "type": "module",
  "files": [
    "dist"
  ],
